local ServerMortis = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()

local Window = ServerMortis:MakeWindow({Name = "Pop it trading", HidePremium = false, SaveConfig = true, ConfigFolder = "Pop it test"})
--Цветки
local tabf = Window:MakeTab({
	Name = "Flower",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})
--Секция (Name название секции)
local secf = tabf:AddSection({
	Name = "Flower"
})
--Торты
local tabc = Window:MakeTab({
	Name = "Cake",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})
--Секция (Name название секции)
local secc = tabc:AddSection({
	Name = "Cake"
})
--Коробки
local tabb = Window:MakeTab({
	Name = "Box Toy",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})
--Секция (Name название секции)
local secb = tabb:AddSection({
	Name = "Box Toy"
})

-- Жёлтый цветок
tabf:AddButton({
	Name = "Buy Yellow Flower",
	Callback = function()
        local args = {
            [1] = "Flower Yellow"
        }
        
        game:GetService("ReplicatedStorage").RemoteEvents.BuyItemCash:FireServer(unpack(args))
  	end    
})
